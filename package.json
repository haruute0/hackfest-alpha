{
    "name": "hackfest",
    "version": "1.0.0",
    "main": "index.js",
    "license": "ISC",
    "scripts": {
        "css-build": "node-sass --omit-source-map-url scss/ -o css/",
        "css-build-minify": "node-sass --omit-source-map-url scss/ -o css/ --output-style compressed",
        "css-watch": "yarn run css-build -- --watch",
        "start": "yarn run css-watch",
        "delete-cache": "rm -rf css/",
        "netlify-deploy": "grunt build",
        "unch": "grunt",
        "compress": "grunt imagemin"
    },
    "private": true,
    "dependencies": {
        "bulma": "^0.7.2",
        "node-sass": "^4.11.0",
        "normalize.css": "^8.0.1"
    },
    "devDependencies": {
        "grunt": "^1.0.3",
        "grunt-browser-sync": "^2.2.0",
        "grunt-cli": "^1.3.2",
        "grunt-contrib-cssmin": "^3.0.0",
        "grunt-contrib-imagemin": "^3.1.0",
        "grunt-contrib-sass": "^1.0.0",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-sass": "^3.0.2",
        "grunt-uncss": "^0.8.5",
        "imagemin-mozjpeg": "^8.0.0",
        "load-grunt-tasks": "^4.0.0",
        "prettier": "1.16.3"
    }
}
